@font-face {
  font-family: 'Tenby Five';
  src: url(<%= asset_path 'tenby-five-webfont.eot' %>);
  src: url(<%= asset_path 'tenby-five-webfont.eot?#iefix' %>) format('embedded-opentype'),
       url(<%= asset_path 'tenby-five-webfont.woff' %>) format('woff'),
       url(<%= asset_path 'tenby-five-webfont.ttf' %>) format('truetype'),
       url(<%= asset_path 'tenby-five-webfont.svg#TenbyFiveRegular' %>) format('svg');
  font-weight: 400;
  font-style: normal;
}

@font-face {
  font-family: 'Tenby Five';
  src: url(<%= asset_path 'tenby-five-light-webfont.eot' %>);
  src: url(<%= asset_path 'tenby-five-light-webfont.eot?#iefix' %>) format('embedded-opentype'),
       url(<%= asset_path 'tenby-five-light-webfont.woff' %>) format('woff'),
       url(<%= asset_path 'tenby-five-light-webfont.ttf' %>) format('truetype'),
       url(<%= asset_path 'tenby-five-light-webfont.svg#TenbyFiveLightRegular' %>) format('svg');
  font-weight: 200;
  font-style: normal;
}
